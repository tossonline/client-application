{
  "Application": {
    "DistributedTracing": {
      "IsEnabled": false,
      "DefaultTags": {
        "Environment": "Staging"
      }
    }
  },
  "LoggerOptions": {
    "Environment": "staging"
  },
  "BackgroundServices": {
    "Dashboard": {
      "Username": "VAULT:affiliates:affiliate_platform/staging/service_auth:username",
      "Password": "VAULT:affiliates:affiliate_platform/staging/service_auth:password"
    },
    "JobConfigurations": {
      "ArchivingJobConfiguration": {
        "AnalyticsArchiveJob": {
          "Source": {
            "Server": "${SQL_HOST}",
            "Username": "${SQL_USERNAME}",
            "Password": "${SQL_PASSWORD}"
          }
        }
      },
      "CleanupJobConfiguration": {
        "AnalyticsArchiveCleanupJob": {
          "Source": {
            "Server": "${SQL_ARCHIVE_HOST}",
            "Username": "${SQL_ARCHIVE_USERNAME}",
            "Password": "${SQL_ARCHIVE_PASSWORD}"
          }
        }
      },
      "MetricsJobConfiguration": {
        "DeadLetterScheduledMetricJob": {
          "Source": {
            "Server": "${SQL_HOST}",
            "Username": "${SQL_USERNAME}",
            "Password": "${SQL_PASSWORD}"
          }
        }
      }
    }
  },
  "Persistence": {
    "Server": "${SQL_HOST}",
    "Username": "${SQL_USERNAME}",
    "Password": "${SQL_PASSWORD}"
  },
  "KafkaConsumers": {
  },
  "KafkaProducers": {
    "LogMessage": {
      "Brokers": [
        "${KAFKA_BROKER}"
      ],
      "Topic": "${KAFKA_TOPIC_LOGGING}",
      "Username": "${KAFKA_USERNAME}",
      "Password": "${KAFKA_PASSWORD}",
      "SchemaRegistryUrl": "${SCHEMA_REGISTRY_URL}"
    },
    "ProduceHealthCheck": {
      "Brokers": [
        "${KAFKA_BROKER}"
      ],
      "Topic": "${KAFKA_TOPIC_HEALTH}",
      "Username": "${KAFKA_USERNAME}",
      "Password": "${KAFKA_PASSWORD}",
      "SchemaRegistryUrl": "${SCHEMA_REGISTRY_URL}"
    }
  }
}