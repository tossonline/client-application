{
  "Application": {
    "DistributedTracing": {
      "IsEnabled": true,
      "Host": "http://localhost",
      "DefaultTags": {
        "Environment": "Development"
      }
    }
  },
  "LoggerOptions": {
    "Environment": "dev"
  },
  "BackgroundServices": {
    "Dashboard": {
      "Username": "VAULT:affiliates:affiliate_platform/development/service_auth:username",
      "Password": "VAULT:affiliates:affiliate_platform/development/service_auth:password"
    },
    "JobConfigurations": {
      "ArchivingJobConfiguration": {
        "AnalyticsArchiveJob": {
          "RetentionPeriodDays": 1,
          "Source": {
             "Server": "${SQL_HOST}",
             "Username": "${SQL_USERNAME}",
             "Password": "${SQL_PASSWORD}"
          }
        }
      },
      "CleanupJobConfiguration": {
        "AnalyticsArchiveCleanupJob": {
          "RetentionPeriodDays": 2,
          "Source": {
             "Server": "${SQL_ARCHIVE_HOST}",
             "Username": "${SQL_ARCHIVE_USERNAME}",
             "Password": "${SQL_ARCHIVE_PASSWORD}"
          }
        }
      },
      "MetricsJobConfiguration": {
        "DeadLetterScheduledMetricJob": {
          "Source": {
             "Server": "${SQL_HOST}",
             "Username": "${SQL_USERNAME}",
             "Password": "${SQL_PASSWORD}"
          }
        }
      }
    }
  },
  "Persistence": {
    "Server": "${SQL_HOST}",
    "Username": "${SQL_USERNAME}",
    "Password": "${SQL_PASSWORD}"
  },
  "kafkaConsumers": {
  },
  "KafkaProducers": {
    "LogMessage": {
      "Brokers": [
        "${KAFKA_BROKER}"
      ],
      "Topic": "${KAFKA_TOPIC_LOGGING}",
      "Username": "${KAFKA_USERNAME}",
      "Password": "${KAFKA_PASSWORD}",
      "SchemaRegistryUrl": "${SCHEMA_REGISTRY_URL}"
    },
    "ProduceHealthCheck": {
      "Brokers": [
        "${KAFKA_BROKER}"
      ],
      "Topic": "${KAFKA_TOPIC_HEALTH}",
      "Username": "${KAFKA_USERNAME}",
      "Password": "${KAFKA_PASSWORD}",
      "SchemaRegistryUrl": "${SCHEMA_REGISTRY_URL}"
    },
    "ProduceCurrencyUpdatedMessage": {
      "Brokers": [
        "${KAFKA_BROKER}"
      ],
      "Topic": "EVT_DOSCPT_IGBAFFILIATES_DEV_AAP_EXT_MASTER-DATA_CURRENCY_CURRENCY-UPDATED",
      "Username": "VAULT:affiliates:affiliate_platform/development/kafka_username:value",
      "Password": "VAULT:affiliates:affiliate_platform/development/kafka_password:value",
      "SchemaRegistryUrl": "https://cptschdev.capetown.fwslash.net"
    }
  }
}